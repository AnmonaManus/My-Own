#include<iostream>
#define max 10
using namespace std;
class stack
{
    int arr[max];
    int top;
    public:
    stack()
    {
        top=-1;
    }
    char push(int x)
    {
        if(top==max-1)
        {
            cout <<"stack is overflow"<<endl;
        }
        else
            {
                top++;
                arr[top]=x;
        cout<<x<<" is pushed"<<endl;
            }
    }
    char pop()
    {
        if (top==-1)
        {
            cout<<"stack is underflow"<<endl;
        }
        else
            {
                top--;
                cout<<arr[top]<<" is poped"<<endl;
            }
    }
};
int main()
{
    stack s;
    char arr1[10];
    int a,b,c;
    for (int i=0;i<10;i++)
    {
        cin>>arr1[i];
    }
    for (int i=0;i<10;i++)
    {
        if(arr1[i]>=0&&arr1[i]<=9)
        {
            s.push(arr1[i]);
        }
       else if(arr1[i]=='+')
        {
            a=s.pop();
            b=s.pop();

            c=a+b;
            s.push(c);
        }
        else if(arr1[i]=='-')
        {
            a=s.pop();
            b=s.pop();

            c=a-b;
            s.push(c);
        }
        else if(arr1[i]=='*')
        {
            a=s.pop();
            b=s.pop();

            c=a*b;
            s.push(c);
        }
        else if(arr1[i]=='/')
        {
            a=s.pop();
            b=s.pop();

            c=a/b;
            s.push(c);
        }
        else if(arr1[i]=='^')
        {
            a=s.pop();
            b=s.pop();

            c=a^b;
            s.push(c);

        }
        else if(arr1[i]=='(')
        {
            break;
        }
        else
                continue;

    }

}
